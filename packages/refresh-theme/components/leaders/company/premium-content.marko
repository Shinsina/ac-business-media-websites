import { getAsArray, getAsObject } from "@mindful-web/object-path";

$ const content = getAsArray(input, "content");


<marko-web-node-list class=input.class>
  <@header>Premium Content</@header>

  <@nodes|{ nodes }| nodes=content>
    <default-theme-card-deck-flow
      cols=1
      nodes=nodes
    >
      <@slot|{ node }|>
        <refresh-theme-leaders-premium-content-card-node node=node />
      </@slot>
    </default-theme-card-deck-flow>
  </@nodes>
  </marko-web-node-list>
